              "margin-bottom": "50px",
              "position": "relative",
              "word-wrap": "break-word",
              "word-break": "break-word",
              "display": "-webkit-flex",
              "flex-direction": "row",
              "-webkit-flex-direction": "row"
            },
            ".iconBackground": {
              "position": "absolute",
              "left": "25%",
              "width": "20px",
              "height": "20px",
              "line-height": "30px",
              "text-align": "center",
              "border-radius": "50%",
              "border": "2px solid @timelineColor",
              "font-size": "30px",
              "display": "inline-block",
              "top": "29px",
              "margin-left": "-10px"
            },
            ".separline:before, .first-separline:before": {
              "top": "3rem",
              "bottom": "0",
              "position": "absolute",
              "content": "\"\"",
              "width": "2px",
              "background-color": "@timelineColor",
              "left": "calc(25% ~\"-\" 1px)",
              "height": "calc(100% ~\"+\" 2rem)"
            },
